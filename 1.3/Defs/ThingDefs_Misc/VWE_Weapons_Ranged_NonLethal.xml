<Defs>


 <!-- ====================== Tesla Gun  ===================== -->
  <ThingDef ParentName="VWENL_Bullet_TaserGeneric" Class="VanillaWeaponsExpandedLaser.LaserBeamDef">
    <defName>VWE_Bullet_Taser</defName>
    <label>taser shot</label>
    <description>A high-voltage taser shot.</description>
    <graphicData>
      <graphicClass>Graphic_Flicker</graphicClass>
      <texPath>Things/Projectile/Shot_TaserGun</texPath>
      <shaderType>TransparentPostLight</shaderType>
      <drawSize>1.5</drawSize>
    </graphicData>
    <textures>
      <li>Things/Projectile/Shot_TaserGun</li>
    </textures>
    <seam>0</seam>
	<flickerFrameTime>5</flickerFrameTime>
    <causefireChance>0</causefireChance>
	
	<beamWidth>4</beamWidth>
	<lifetime>120</lifetime>

    <projectile>
		<damageDef>Stun</damageDef>
		<damageAmountBase>46</damageAmountBase>
		<armorPenetrationBase>0.45</armorPenetrationBase>
		<stoppingPower>3</stoppingPower>
    </projectile>
  </ThingDef>
  
 <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>VWE_Gun_Taser</defName>
	<thingClass>VanillaWeaponsExpandedLaser.LaserGun</thingClass>
    <label>taser gun</label>
    <description>An industrial tech short range taser pistol. It delivers an electric shock aimed at temporarily disrupting muscle functions and inflicting pain without usually causing significant injury.</description>
    <graphicData>
		<texPath>Things/Item/Equipment/WeaponRanged/TaserGun</texPath>
		<graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Autopistol</soundInteract>
    <statBases>
		<WorkToMake>4600</WorkToMake>
		<MarketValue>800</MarketValue>
		<Mass>1.2</Mass>
			<AccuracyTouch>1</AccuracyTouch>
			<AccuracyShort>0.89</AccuracyShort>
			<AccuracyMedium>0.78</AccuracyMedium>
			<AccuracyLong>0.6</AccuracyLong>
			<RangedWeapon_Cooldown>6.6</RangedWeapon_Cooldown>
    </statBases>
    <costList>
		<Steel>45</Steel>
		<ComponentIndustrial>2</ComponentIndustrial>
    </costList>
    <recipeMaker>
      <skillRequirements>
        <Crafting>10</Crafting>
      </skillRequirements>
      <researchPrerequisite Inherit="False">VWE_NonLethal</researchPrerequisite>
    </recipeMaker>
	<techLevel>Industrial</techLevel>
    <weaponTags Inherit="False">
		<li>NonLethal</li>
    </weaponTags>
		<generateCommonality>0.1</generateCommonality>
    <verbs>
		<li>
			<verbClass>Verb_Shoot</verbClass>
			<label>Standard</label>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>VWE_Bullet_Taser</defaultProjectile>
			<warmupTime>1.2</warmupTime>
			<range>6.9</range>
			<ticksBetweenBurstShots>8</ticksBetweenBurstShots>
			<burstShotCount>1</burstShotCount>
			<soundCast>VWE_TaserShot_Electricity</soundCast>
			<soundCastTail>GunTail_Heavy</soundCastTail>
			<muzzleFlashScale>14</muzzleFlashScale>
			<targetParams>
				<canTargetLocations>false</canTargetLocations>
			</targetParams>
      </li>
    </verbs>
    <tools>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>1.8</cooldownTime>
      </li>
      <li>
        <label>grip</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.9</cooldownTime>
      </li>
    </tools>
        <modExtensions>
	            <li Class="VFECore.ThingDefExtension">
                <usableWithShields>true</usableWithShields>
            </li>
        </modExtensions>
 </ThingDef>
 
